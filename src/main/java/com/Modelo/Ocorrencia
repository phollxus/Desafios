package com.desafio.projeto1.modelo;

import lombok.*;

import  javax.persistence.Entity;
import javax.persistence.Id;

@Getter
@Setter
@AllArgsConstructor
@NoArgsConstructor
@EqualsAndHashcode
@Builder
@Entity

public class Ocorrencia {
    @Id
    private long id;
    private String nome;
    private String descricao;
}